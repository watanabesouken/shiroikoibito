.p-hamburger {
  display: block;
  position: absolute;
  z-index: 3;
  right: 0;
  top: 0;
  width: 56px;
  height: 56.95px;
  cursor: pointer; }

.p-hamburger span {
  height: 2px;
  position: absolute;
  width: 2rem;
  background: #707070;
  -webkit-transition: 0.3s ease-in-out;
  transition: 0.3s ease-in-out;
  left: 29%; }

.p-hamburger span:nth-child(1) {
  top: 2rem; }

.p-hamburger span:nth-child(2) {
  top: 2.7rem; }

.p-hamburger span:nth-child(3) {
  top: 3.4rem; }

/* ナビ開いてる時に矢印にする */
.p-hamburger.active span:nth-child(1),
.p-hamburger.active span:nth-child(3) {
  width: 26px;
  background: #fff; }

.p-hamburger.active span:nth-child(1) {
  -webkit-transform: translate(-1px, 5px) rotate(-45deg);
  transform: translate(-1px, 5px) rotate(-45deg);
  top: 2.3rem; }

.p-hamburger.active span:nth-child(2) {
  display: none; }

.p-hamburger.active span:nth-child(3) {
  -webkit-transform: translate(-1px, -5px) rotate(45deg);
  transform: translate(-1px, -5px) rotate(45deg); }

