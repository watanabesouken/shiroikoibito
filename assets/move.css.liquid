.fadeY{
	animation: FadeY 1s ease-in 1.7s;
	animation-fill-mode: forwards;
	opacity: 0;
}
.fade{
	animation: Fade 0.5s ease-in 2.5s;
	animation-fill-mode: forwards;
	opacity: 0;
}
.fade-after,.fadeY-after{
	opacity: 0;
}
.fade-after.effect-scroll{
	animation: Fade 0.5s ease-in 0.5s;
	animation-fill-mode: forwards;
}
.fadeY-after.effect-scroll{
	animation: FadeY 0.5s ease-in 0.5s;
	animation-fill-mode: forwards;
}
.sway-after,.fallin-after,.sway-s-after{
	opacity: 0;
}
.sway,.sway-after.effect-scroll{
	opacity: 0;
	animation: Fade 1s linear forwards ,Sway 2.4s linear infinite 1.5s;
}

.sway-s-after.effect-scroll{
	opacity: 0;
	animation: Fade 1s linear forwards ,SwayS 2.4s linear infinite 1.5s;
}

.fallin,.fallin-after.effect-scroll{
	animation: FallIn 0.8s linear;
  	animation-fill-mode: forwards;
  	opacity:0;
}
.fade-after-c{
	opacity:0;
}
.fade-after-p.effect-scroll .fade-after-c{
  	animation: FadeY 0.5s ease-in 0.5s;
	animation-fill-mode: forwards;
}

@keyframes FadeY {
  0% {
    opacity: 0;
    transform: translateY(10px);
  }
  100% {
    opacity: 1;
    transform: translateY(0);
  }
}
@keyframes FadeY2 {
  0% {
    opacity: 0;
    transform: translateY(50px);
  }
  70%{
    transform: translateY(-8px);
  }
  100% {
    opacity: 1;
    transform: translateY(0);
  }
}
@keyframes Fade {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
@keyframes Grow {
  0% {
    opacity: 0;
    transform: translateY(20%)scale(0.5);
  }
  100% {
    opacity: 1;
    transform: translateY(0)scale(1);
  }
}
@keyframes Sway {
  0% {
    transform: translate(0, 45%)rotate(0)scaleY(0.99)translate(0, -45%);
  }
  25% {
    transform: translate(0, 45%)rotate(2deg)translate(0, -45%);
  }
  50% {
    transform: translate(0, 45%)rotate(0)scaleY(0.99)translate(0, -45%);
  }
  75% {
    transform: translate(0, 45%)rotate(-2deg)translate(0, -45%);
  }
  100% {
    transform: translate(0, 45%)rotate(0)scaleY(0.99)translate(0, -45%);
  }
}
@keyframes SwayS {
  0% {
    transform: translate(0, 45%)rotate(0)scaleY(0.99)translate(0, -45%);
  }
  25% {
    transform: translate(0, 45%)rotate(5deg)translate(0, -45%);
  }
  50% {
    transform: translate(0, 45%)rotate(0)scaleY(0.99)translate(0, -45%);
  }
  75% {
    transform: translate(0, 45%)rotate(-5deg)translate(0, -45%);
  }
  100% {
    transform: translate(0, 45%)rotate(0)scaleY(0.99)translate(0, -45%);
  }
}


@keyframes FallIn {
  0% {
    opacity: 0;
    transform: translateY(-350px);
  }
  40% {
    opacity: 0.5;
    transform: translateY(0);
  }
  50% {
    opacity: 1;
    transform: translateY(0);
  }
  55% {
    transform: translateY(-10px);
  }
  70% {
    transform: translateY(0);
  }
  85% {
    transform: translateY(-5px);
  }
  100%{
  	transform: translateY(0);
  	opacity: 1;
  }
}

/*delay*/
.fallin-delay1{
	animation-delay: 1s;
}
.fallin-delay2{
	animation-delay: 1.2s;
}
.fallin-delay3{
	animation-delay: 1.3s;	
}
.fallin-delay4{
	animation-delay: 1.4s;
}
.fallin-delay5{
	animation-delay: 1.5s;	
}
.fallin-delay6{
	animation-delay: 1.6s;
}

.move__tree2{
	animation: Fade 1s linear forwards ,Sway 2.4s linear infinite 2.16s;
}
.move__tree3{
	animation: Fade 1s linear forwards ,Sway 2.4s linear infinite 2.43s;
}
.move__tree4{
	animation: Fade 1s linear forwards ,Sway 2.4s linear infinite 2.69s;
}

