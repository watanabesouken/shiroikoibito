	animation: FadeY2 0.6s ease-out 0.8s;
	animation-fill-mode: forwards;
}
.barrage-p.effect-scroll .barrage-c:nth-child(3){
	animation: FadeY2 0.6s ease-out 1s;
	animation-fill-mode: forwards;
}
.barrage-p.effect-scroll .barrage-c:nth-child(4){
	animation: FadeY2 0.6s ease-out 1.2s;
	animation-fill-mode: forwards;
}
.barrage-p.effect-scroll .barrage-c-after:nth-child(1){
	animation: FadeY2 0.6s ease-out 1.8s;
	animation-fill-mode: forwards;
}
.barrage-p.effect-scroll .barrage-c-after:nth-child(2){
	animation: FadeY2 0.6s ease-out 2s;
	animation-fill-mode: forwards;
}
.barrage-p.effect-scroll .barrage-c-after:nth-child(3){
	animation: FadeY2 0.6s ease-out 2.2s;
	animation-fill-mode: forwards;
}
.barrage-p.effect-scroll .barrage-c-after:nth-child(4){
	animation: FadeY2 0.6s ease-out 2.4s;
	animation-fill-mode: forwards;
}

.barrage-p.effect-scroll .barrage-c-fade:nth-child(1){
	animation: Fade 0.4s ease-out 0.4s;
	animation-fill-mode: forwards;
}
.barrage-p.effect-scroll .barrage-c-fade:nth-child(2){
	animation: Fade 0.4s ease-out 0.6s;
	animation-fill-mode: forwards;
}
.barrage-p.effect-scroll .barrage-c-fade:nth-child(3){
	animation: Fade 0.4s ease-out  0.8s;
	animation-fill-mode: forwards;
}
.barrage-p.effect-scroll .barrage-c-fade:nth-child(4){
	animation: Fade 0.4s ease-out  1.2s;
	animation-fill-mode: forwards;
}

